{
  "beatPulse": {
    "metadata": {
      "name": "Beat Pulse",
      "bpm": 120,
      "duration": 60
    },
    "settings": {
      "audioReactive": {
        "amplitudeMultiplier": 1.5,
        "velocityThreshold": 0.15
      },
      "collisionBehavior": "default",
      "boundaryMode": "bounce"
    },
    "timeline": [
      {
        "trigger": {
          "type": "loop",
          "every": 0.5,
          "count": -1
        },
        "actions": [
          {
            "type": "spawn",
            "objectId": "pulse-{{index}}",
            "template": "star",
            "position": {
              "x": "random",
              "y": "center"
            }
          },
          {
            "type": "transform",
            "target": "pulse-{{index}}",
            "effect": "EXPLODE",
            "duration": 2.0
          }
        ]
      }
    ]
  },

  "audioReactiveFormation": {
    "metadata": {
      "name": "Audio Reactive Formation Dance",
      "duration": 120
    },
    "templates": {
      "objects": {
        "dancer": {
          "shape": "star",
          "defaultColor": "#FF00FF",
          "defaultScale": 1.0,
          "physics": {
            "mass": 1.0,
            "friction": 0.1,
            "elasticity": 0.8
          }
        }
      }
    },
    "timeline": [
      {
        "trigger": {
          "type": "time",
          "at": 0
        },
        "actions": [
          {
            "type": "spawn",
            "objectId": "obj1",
            "template": "dancer",
            "position": {"x": 20, "y": 12}
          },
          {
            "type": "spawn",
            "objectId": "obj2",
            "template": "dancer",
            "position": {"x": 30, "y": 12}
          },
          {
            "type": "spawn",
            "objectId": "obj3",
            "template": "dancer",
            "position": {"x": 50, "y": 12}
          },
          {
            "type": "spawn",
            "objectId": "obj4",
            "template": "dancer",
            "position": {"x": 60, "y": 12}
          }
        ]
      },
      {
        "trigger": {
          "type": "time",
          "at": 2
        },
        "actions": [
          {
            "type": "formation",
            "objects": ["obj1", "obj2", "obj3", "obj4"],
            "pattern": "circle",
            "center": {"x": 40, "y": 12},
            "duration": 2.0
          }
        ]
      },
      {
        "trigger": {
          "type": "audio",
          "condition": {
            "parameter": "velocity",
            "operator": "spike",
            "value": 0.3
          }
        },
        "actions": [
          {
            "type": "transform",
            "target": "all",
            "effect": "RAINBOW",
            "duration": 0.5
          }
        ]
      },
      {
        "trigger": {
          "type": "audio",
          "condition": {
            "parameter": "amplitude",
            "operator": ">",
            "value": 0.8
          }
        },
        "actions": [
          {
            "type": "visual",
            "target": "all",
            "changes": {
              "scale": 2.0,
              "glow": {
                "color": "#FFFF00",
                "radius": 3
              }
            },
            "duration": 0.2,
            "easing": "bounce"
          }
        ]
      }
    ]
  },

  "complexSequence": {
    "metadata": {
      "name": "Complex Choreographed Sequence",
      "bpm": 140,
      "timeSignature": "4/4",
      "duration": 180
    },
    "tracks": [
      {
        "id": "foreground",
        "name": "Main Characters",
        "layer": 3,
        "opacity": 1.0
      },
      {
        "id": "midground",
        "name": "Supporting Elements",
        "layer": 2,
        "opacity": 0.8
      },
      {
        "id": "background",
        "name": "Ambient Particles",
        "layer": 1,
        "opacity": 0.5
      }
    ],
    "scenes": [
      {
        "name": "Introduction",
        "start": 0,
        "duration": 10,
        "timeline": [
          {
            "trigger": {
              "type": "beat",
              "measure": 1,
              "beat": 1
            },
            "actions": [
              {
                "type": "spawn",
                "objectId": "main-character",
                "position": {"x": 0, "y": "center"},
                "track": "foreground"
              },
              {
                "type": "move",
                "target": "main-character",
                "movement": {
                  "path": [
                    {"x": 0, "y": 12, "time": 0},
                    {"x": 40, "y": 6, "time": 2},
                    {"x": 80, "y": 12, "time": 4}
                  ]
                }
              }
            ]
          }
        ]
      },
      {
        "name": "Build Up",
        "start": 10,
        "duration": 20,
        "timeline": [
          {
            "trigger": {
              "type": "loop",
              "every": 0.25,
              "count": 40
            },
            "actions": [
              {
                "type": "spawn",
                "objectId": "particle-{{index}}",
                "template": "dot",
                "position": {
                  "x": "random",
                  "y": "random"
                },
                "track": "background"
              },
              {
                "type": "audio-map",
                "target": "particle-{{index}}",
                "mapping": {
                  "amplitude": {
                    "property": "scale",
                    "range": [0.5, 2.0],
                    "smoothing": 0.1
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "name": "Climax",
        "start": 30,
        "duration": 10,
        "timeline": [
          {
            "trigger": {
              "type": "time",
              "at": 30
            },
            "actions": [
              {
                "type": "transform",
                "target": "all",
                "effect": "MATRIX",
                "duration": 1.0
              }
            ]
          },
          {
            "trigger": {
              "type": "time",
              "at": 32
            },
            "actions": [
              {
                "type": "formation",
                "objects": ["main-character"],
                "pattern": "spiral",
                "center": {"x": 40, "y": 12},
                "duration": 5.0
              }
            ]
          }
        ]
      }
    ]
  },

  "physicsParticles": {
    "metadata": {
      "name": "Physics-Based Particle System",
      "duration": 60
    },
    "settings": {
      "audioReactive": {
        "amplitudeMultiplier": 2.0
      },
      "boundaryMode": "destroy"
    },
    "templates": {
      "objects": {
        "dot": {
          "shape": "â€¢",
          "defaultColor": "#00FF00",
          "physics": {
            "mass": 0.1,
            "friction": 0.05,
            "elasticity": 0.9
          }
        }
      }
    },
    "timeline": [
      {
        "trigger": {
          "type": "audio",
          "condition": {
            "parameter": "amplitude",
            "operator": ">",
            "value": 0.7
          }
        },
        "actions": [
          {
            "type": "spawn",
            "objectId": "particle-{{timestamp}}",
            "template": "dot",
            "position": {
              "x": "center",
              "y": "bottom"
            }
          },
          {
            "type": "move",
            "target": "particle-{{timestamp}}",
            "movement": {
              "velocity": {
                "x": "random(-5,5)",
                "y": -10
              },
              "acceleration": {
                "x": 0,
                "y": 0.5
              }
            }
          },
          {
            "type": "visual",
            "target": "particle-{{timestamp}}",
            "changes": {
              "color": "hsl(random(0,360), 100%, 50%)"
            },
            "duration": 0
          }
        ]
      }
    ]
  },

  "frequencyVisualization": {
    "metadata": {
      "name": "Frequency Band Visualization",
      "duration": 120
    },
    "settings": {
      "audioReactive": {
        "frequencyBands": [
          {
            "name": "bass",
            "range": [20, 250],
            "weight": 1.5
          },
          {
            "name": "mid",
            "range": [250, 2000],
            "weight": 1.0
          },
          {
            "name": "treble",
            "range": [2000, 20000],
            "weight": 0.8
          }
        ]
      }
    },
    "timeline": [
      {
        "trigger": {
          "type": "time",
          "at": 0
        },
        "actions": [
          {
            "type": "spawn",
            "objectId": "bass-bar",
            "template": "bar",
            "position": {"x": 20, "y": 20}
          },
          {
            "type": "spawn",
            "objectId": "mid-bar",
            "template": "bar",
            "position": {"x": 40, "y": 20}
          },
          {
            "type": "spawn",
            "objectId": "treble-bar",
            "template": "bar",
            "position": {"x": 60, "y": 20}
          }
        ]
      },
      {
        "trigger": {
          "type": "time",
          "at": 0.1
        },
        "actions": [
          {
            "type": "audio-map",
            "target": "bass-bar",
            "mapping": {
              "frequency": {
                "band": "bass",
                "property": "scale",
                "range": [0.5, 5.0]
              }
            }
          },
          {
            "type": "audio-map",
            "target": "mid-bar",
            "mapping": {
              "frequency": {
                "band": "mid",
                "property": "scale",
                "range": [0.5, 3.0]
              }
            }
          },
          {
            "type": "audio-map",
            "target": "treble-bar",
            "mapping": {
              "frequency": {
                "band": "treble",
                "property": "scale",
                "range": [0.5, 2.0]
              }
            }
          }
        ]
      }
    ]
  }
}